@using Restaurant.ViewModels
@model HomeViewModel
<!-- Hero Section
====================================== -->
<section id="slider" class="slider slide-overlay-black">
	<!-- START REVOLUTION SLIDER 5.0 -->
	<div class="rev_slider_wrapper">
		<div id="slider1" class="rev_slider"  data-version="5.0">
            <ul>
                <li data-transition="zoomin"
                    data-slotamount="default"
                    data-easein="Power4.easeInOut"
                    data-easeout="Power4.easeInOut"
                    data-masterspeed="2000">
                    <!-- MAIN IMAGE -->
                    <img src="~/assets/images/sliders/slide-bg/11.jpg" alt="Slide Background Image" width="1920" height="1280">
                    <!-- LAYER NR. 1 -->
                    <div class="tp-caption"
                         data-x="center" data-hoffset="0"
                         data-y="center" data-voffset="-130"
                         data-whitespace="nowrap"
                         data-width="none"
                         data-height="none"
                         data-frames='[{"delay":1500,"speed":1000,"frame":"0","from":"y:-50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"auto:auto;","ease":"Power3.easeInOut"}]' data-splitin="none"
                         data-splitout="none"
                         data-responsive_offset="on">
                        <div class="slide--subheadline">Odyssey Offers You</div>
                    </div>


                    <!-- LAYER NR. 2 -->
                    <div class="tp-caption"
                         data-x="center" data-hoffset="0"
                         data-y="center" data-voffset="-65"
                         data-whitespace="nowrap"
                         data-width="none"
                         data-height="none"
                         data-frames='[{"delay":1750,"speed":1000,"frame":"0","from":"y:50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"opacity:0;","ease":"Power3.easeInOut"}]' data-splitin="none"
                         data-splitout="none"
                         data-responsive_offset="on">
                        <div class="slide--headline">The Best Tasting Experience!</div>
                    </div>

                    <!-- LAYER NR. 3 -->
                    <div class="tp-caption"
                         data-x="center" data-hoffset="0"
                         data-y="center" data-voffset="20"
                         data-width="none"
                         data-height="none"
                         data-frames='[{"delay":2000,"speed":1500,"frame":"0","from":"y:50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"opacity:0;","ease":"Power3.easeInOut"}]'
                         data-splitin="none"
                         data-splitout="none"
                         data-responsive_offset="on">
                        <div class="slide--bio text-center">Odyssey is a restaurant, bar and coffee roastery located on Egypt. We have<br> awesome recipes and the most talented chefs in town! </div>
                    </div>

                    <!-- LAYER NR. 4 -->
                    <div class="tp-caption"
                         data-x="center" data-hoffset="0"
                         data-y="center" data-voffset="100"
                         data-width="none"
                         data-height="none"
                         data-whitespace="nowrap"
                         data-frames='[{"delay":2250,"speed":1000,"frame":"0","from":"x:-50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"auto:auto;","ease":"Power3.easeInOut"}]' data-splitin="none"
                         data-splitout="none"
                         data-actions='[{"event":"click","action":"jumptoslide","slide":"rs-164","delay":""}]'
                         data-basealign="slide"
                         data-responsive_offset="on"
                         data-responsive="off">
                        <div class="slide-action">
                            <a class="btn btn--white btn--bordered btn--lg" asp-controller="Reservation" asp-action="Create" asp-area="Admin">Get A Reservation Now</a>
                        </div>
                    </div>
                </li>
                @foreach(var item in Model.Slides)
                {
                    <!-- slide 1 -->
                    <li data-transition="zoomin"
                        data-slotamount="default"
                        data-easein="Power4.easeInOut"
                        data-easeout="Power4.easeInOut"
                        data-masterspeed="2000">
                        <!-- MAIN IMAGE -->
                        <img src="~/uploads/Sliders/@item.ImageUrl" alt="Slide Background Image" width="1920" height="1280">
                        <!-- LAYER NR. 1 -->
                        <div class="tp-caption"
                             data-x="center" data-hoffset="0"
                             data-y="center" data-voffset="-130"
                             data-whitespace="nowrap"
                             data-width="none"
                             data-height="none"
                             data-frames='[{"delay":1500,"speed":1000,"frame":"0","from":"y:-50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"auto:auto;","ease":"Power3.easeInOut"}]' data-splitin="none"
                             data-splitout="none"
                             data-responsive_offset="on">
                            <div class="slide--subheadline">@item.Subtitle</div>
                        </div>


                        <!-- LAYER NR. 2 -->
                        <div class="tp-caption"
                             data-x="center" data-hoffset="0"
                             data-y="center" data-voffset="-65"
                             data-whitespace="nowrap"
                             data-width="none"
                             data-height="none"
                             data-frames='[{"delay":1750,"speed":1000,"frame":"0","from":"y:50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"opacity:0;","ease":"Power3.easeInOut"}]' data-splitin="none"
                             data-splitout="none"
                             data-responsive_offset="on">
                            <div class="slide--headline">@item.Title</div>
                        </div>

                        <!-- LAYER NR. 3 -->
                        <div class="tp-caption"
                             data-x="center" data-hoffset="0"
                             data-y="center" data-voffset="20"
                             data-width="none"
                             data-height="none"
                             data-frames='[{"delay":2000,"speed":1500,"frame":"0","from":"y:50px;opacity:0;","to":"o:1;","ease":"Power3.easeInOut"},{"delay":"wait","speed":300,"frame":"999","to":"opacity:0;","ease":"Power3.easeInOut"}]'
                             data-splitin="none"
                             data-splitout="none"
                             data-responsive_offset="on">
                            <div class="slide--bio text-center">@item.Description </div>
                        </div>
                    </li>
                }
                <!-- slide 1 -->
			</ul>
		</div>
		<!-- END REVOLUTION SLIDER -->
	</div>
	<!-- END OF SLIDER WRAPPER -->
</section>
