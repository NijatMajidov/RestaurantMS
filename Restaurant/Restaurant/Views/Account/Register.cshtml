                        </section>
                        <!-- Title End -->

                        <div>
                            <div class="card mb-5">
                                <div class="card-body">
                                    <p class="mb-0">Examples and usage guidelines for form control styles and layout options.</p>
                                </div>
                            </div>
                            <section class="scroll-section" id="formRow">
                                <h2 class="small-title">Form Row</h2>
                                <div class="card mb-5">
                                    <div class="card-body">
                                        <form method="post" class="row g-3">
                                            <div class="col-md-6">
                                                <label asp-for="Name" class="form-label">Name</label>
                                                <input asp-for="Name" class="form-control" placeholder="First Name">
                                                <span asp-validation-for="Name" style="color:red"></span>
                                            </div>
                                            <div class="col-md-6">
                                                <label asp-for="Surname" class="form-label">Surname</label>
                                                <input asp-for="Surname" class="form-control" id="inputPassword4" placeholder="Last Name">
                                                <span asp-validation-for="Surname" style="color:red"></span>
                                            </div>
                                            <div class="col-12">
                                                <label asp-for="Email" class="form-label">Email Address</label>
                                                <input asp-for="Email" class="form-control" id="inputEmail4" placeholder="email@gmail.com">
                                                <span asp-validation-for="Email" style="color:red"></span>
                                            </div>
                                            <div class="col-12">
                                                <label asp-for="UserName" class="form-label">UserName</label>
                                                <input asp-for="UserName" class="form-control" id="inputAddress2" placeholder="userName">
                                                <span asp-validation-for="UserName" style="color:red"></span>
                                            </div>

                                            <div class="col-md-6">
                                                <label asp-for="Password" class="form-label">Password</label>
                                                    <input asp-for="Password" class="form-control"
                                                       id="inputPassword4" placeholder="Password">
                                                    <span asp-validation-for="Password" style="color:red"></span>
                                            </div>
                                            <div class="col-md-6">
                                                <label asp-for="ConfirmPassword" class="form-label">Confirm Password</label>
                                                    <input asp-for="ConfirmPassword" class="form-control"
                                                       id="inputPassword4" placeholder="Repeat Password">
                                                    <span asp-validation-for="ConfirmPassword" style="color:red"></span>
                                            </div>
                                                <div asp-validation-summary="ModelOnly"></div>
                                            <div class="col-12">
                                                <button type="submit" class="btn btn-primary">Sign in</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </section>

                        </div>
                        <!-- Content End -->
                    </div>
                </div>
            </div>
        </main>
